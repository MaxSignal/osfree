default 0

borderoff 1
indexoff 1
menutitleoff 1

title Virtualizing Linux on small kernel systems
kernel (nd)/tftpboot/nf2/demo/common/bootstrap
modaddr 0x02000000
module (nd)/tftpboot/nf2/demo/common/fiasco nowait -nokdb  -noserial -jdb_never_stop
module (nd)/tftpboot/nf2/demo/common/sigma0
module (nd)/tftpboot/nf2/demo/common/roottask -configfile -symbols -lines task modname "bmodfs" attached 5 modules
module (nd)/tftpboot/nf2/demo/l4lx/roottask.config
module (nd)/tftpboot/nf2/demo/l4lx/fiasco_symbols -noserial -jdb_never_stop
module (nd)/tftpboot/nf2/demo/l4lx/fiasco_lines -noserial -jdb_never_stop
module (nd)/tftpboot/nf2/demo/common/events
module (nd)/tftpboot/nf2/demo/common/names --events
module (nd)/tftpboot/nf2/demo/common/log --prio 0xA1
module (nd)/tftpboot/nf2/demo/common/dm_phys --events
module (nd)/tftpboot/nf2/demo/common/simple_ts -t 800 --events
module (nd)/tftpboot/nf2/demo/common/rtc
module (nd)/tftpboot/nf2/demo/common/l4io
module (nd)/tftpboot/nf2/demo/common/l4exec
module (nd)/tftpboot/nf2/demo/common/nitpicker
module (nd)/tftpboot/nf2/demo/common/nitdope
module (nd)/tftpboot/nf2/demo/common/usbhid
module (nd)/tftpboot/nf2/demo/common/backdrop
module (nd)/tftpboot/nf2/demo/common/bmodfs
module (nd)/tftpboot/nf2/demo/common/libloader.s.so
module (nd)/tftpboot/nf2/demo/l4lx/l4linux_tutorial
module (nd)/tftpboot/nf2/demo/l4lx/l4linux26.cfg
module (nd)/tftpboot/nf2/demo/l4lx/vmlinuz26.ux
module (nd)/tftpboot/nf2/demo/l4lx/image-bb
module (nd)/tftpboot/nf2/demo/common/loader --events --fprov=BMODFS "l4linux_tutorial --fprov BMODFS"
vbeset 0x117

